{
  "manifest_version": 3,
  "name": "Colonist assistant",
  "description": "Keep count of player resources by log inspection",
  "author":"santiago93echevarria@gmail.com",
    "permissions": [
      "cookies",
      "storage",
      "webRequest"
    ],
    "version": "1.0.0",
    "content_scripts": [{
        "matches": ["https://colonist.io/*"],
        "js": ["count.js"]
      }],
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
} 

