{
    "manifest_version": 3,
    "name": "Colonist.io 1v1 resource counter",
    "description": "Keep count of player resources by log inspection",
    "author":"Santiago Echevarria",
      "permissions": [
        "cookies",
        "storage",
        "webRequest"
      ],
      "version": "3.0.0",
      "content_scripts": [{
          "matches": ["https://colonist.io/*"],
          "js": ["main.js"],
          "css": ["style.css"]
        }],
      "icons": {
          "16": "icon.png",
          "32": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
        }
  } 