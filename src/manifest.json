{
  "manifest_version": 3,
  "name": "Colonist log parser",
  "description": "Display parsed log information.",
  "author":"santiago93echevarria@gmail.com",
  "permissions": [  "storage"],
  "version": "2.0",
  "content_scripts": [{
      "matches": ["https://colonist.io/*"],
      "js": ["main.js"],
      "css": ["style.css"]
    
    }],
  "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "web_accessible_resources": [
      {"resources": ["templates/*"],
        "matches": ["https://colonist.io/*"]}
    ]

} 

